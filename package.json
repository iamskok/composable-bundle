{
  "name": "composable-bundle",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:uk": "vite --config vite.uk.ts",
    "dev:us": "vite --config vite.us.ts",
    "build": "tsc && vite build",
    "build:uk": "tsc && vite build --config vite.uk.ts",
    "build:us": "tsc && vite build --config vite.us.ts",
    "preview": "vite preview",
    "preview:uk": "vite preview --config vite.uk.ts",
    "preview:us": "vite preview --config vite.us.ts",
    "update-theme": "ts-node-esm ./scripts/build-time-theme.ts",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "clean-vite-cache": "rm -rf node_modules/.vite"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "react": "^18.2.0",
    "react-confetti": "^6.1.0",
    "react-dom": "^18.2.0",
    "ts-node": "^10.9.1",
    "unplugin-preprocessor-directives": "^0.0.5"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "postcss": "^8.4.30",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  }
}
